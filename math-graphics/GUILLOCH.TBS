10 DEG :GRAPHICS 8+16:COLOR 1
20 XRADIUS=60:YRADIUS=40
30 CX=160:CY=96
40 FOR SCALE=1 TO 45 STEP 2
50   FOR ANGLE=1 TO 360
60     X=(XRADIUS+SCALE*SIN(10*ANGLE))*COS(ANGLE)
70     Y=(YRADIUS+SCALE*SIN(7*ANGLE))*SIN(ANGLE)
80     PLOT CX+X,CY+Y
90   NEXT ANGLE
100 NEXT SCALE
110 TEXT 287,183,"DONE"
120 GOTO 120
